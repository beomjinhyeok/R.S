enum Pedi {
	High_card = 1, // 보드와 무관한 카드 숫자가 높은 사람이 이김
	  //카드 숫자가 모두 똑같으면 비김 // 1
//문양따라가는걸로 순위 매김
One_pair, // 같은숫자 2개 1쌍 // 2
Two_pair, // 같은 숫자 2개 2쌍 // 3
Triple, //같은숫자 3개 // 4
Straight, //연속된 숫자 5개 // 5
Flush, //같은문양 5개 // 6
Full_house, // 트리플 + 원페어 // 7
Four_card, // 같은숫자 4개 // 8
Straight_flush //같은문양, 숫자 오름차순 5개 // 9
};
